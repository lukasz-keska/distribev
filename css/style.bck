body 
{
    font-family: 'Lato', sans-serif;
}
h1 
{
    margin: 0;
    padding: 0;
    color: #303361;
    font-weight: 500;
    text-transform: uppercase;
    font-size: 28px;
    margin-bottom: 35px;
}



.top-navigation 
{
    margin-top: 20px;
    margin-bottom: 25px;
    z-index: 100;
    position:absolute;
    width:100%;
}

.fix{
    margin-top: 0px;
    margin-left: 0px;
    margin-right: 0px;
    position:fixed;
    top:0;
    left:0;
    right:0;
    margin:0;
}

.top-navigation>.container{
    background-color: #161821;
    float: none;
    margin: 0 auto;
    width:100%;
}

.top-navigation:not(.fix)>.container{
    width: 98%;
}

.top-navigation .logo 
{
    margin-top: 10px;
    margin-bottom: 10px;
}
.top-navigation ul
{
    list-style: none;
    margin: 0;
    padding: 0;
    position: relative;
}

.top-navigation ul.main-ul{
    top: 20px;
}

.top-navigation ul li
{
    display: inline-block;
    text-align: left;
    margin-right: 15px;
}

.top-navigation .submenu-block{
    display: block;
    text-align: left;
    margin-right: 15px;
}

.top-navigation ul li:last-child
{
    margin-right: 0;
}
.top-navigation ul li a 
{
    text-decoration: none;
    display: inline-block;
}
.top-navigation ul li a .link-title
{
    text-transform: uppercase;
    color: #FFFFFF;   
    font-size: 13px;
    display: block;
    font-weight: 500;
    margin-bottom: 15px;
    padding-bottom: 15px;
    padding-right: 50px;
    /*border-bottom: 1px solid #484848;*/
}
.top-navigation ul li a:hover .link-title
{
    color: #96d4f2;
    /*text-transform: uppercase;
    font-size: 14px;
    display: block;
    font-weight: 700;
    margin-bottom: 15px;
    padding-bottom: 15px;
    padding-right: 50px;*/
    /*border-bottom: 1px solid #484848;*/
}
.top-navigation ul li a .link-description
{
    color: #484848;
    font-size: 12px;
    display: block;
    font-weight: 500;
    padding-right: 50px;
    display:none;
}
.top-navigation ul .our-brands 
{
    background: #303361;
    /*height: 60px;*/
    width: 200px;
    position: absolute;
    top: 35px;
    right: 305px;
    display: none;
    z-index: 3;
    border-top: 1px solid #FFF;
}
.top-navigation .our-brands ul 
{
    padding-left: 30px;
    /*display: table;*/
    width: 100%;
}
.top-navigation .our-brands ul li
{
    display: block;
