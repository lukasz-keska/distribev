        padding-right: 25px;
    }
    .top-navigation .our-brands ul li
    {
        padding-right: 25px;
    }
    .middle-center img
    {
        max-width: 150px;
    }
}
@media screen and (max-width: 992px)
{
    footer .footer-wrapper ul
    {
        padding-left: 0;
    }
    .top-navigation ul li a .link-description
    {
        padding-right: 0px;
    }
    .top-navigation .our-brands ul li
    {
        padding-right: 4px;
    }
    .top-navigation ul li
    {
        margin-right: 4px;
    }
    .top-navigation .our-brands ul 
    {
        padding-left: 5px;
    }
}
